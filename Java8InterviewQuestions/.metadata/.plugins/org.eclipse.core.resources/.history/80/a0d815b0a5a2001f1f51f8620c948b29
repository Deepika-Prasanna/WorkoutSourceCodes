// To find the second highest number from the given int array

package com.deepu.java8.interview.questions;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class SecondHighestNumber {

	public static void main(String[] args) {
		
		int[] numbers = {5, 9 11, 2, 8, 21, 1};
		 	
		List<Integer> sortedIntegerArray = Arrays.stream(numbers).boxed().sorted().collect(Collectors.toList());
		
		System.out.println(sortedIntegerArray);
	}
}
